{
    "name": "Vintage Story",
    "id": "vintagestory",
    "packaging_format": 1,
    "description": {
        "en": "Vintage Story package for YunoHost.",
        "fr": "Vintage Story pour YunoHost."
    },
    "version": "1.9.0-RC4",
    "url": "https://www.vintagestory.at/",
    "license": "non-free",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 2.7.2"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php5-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for Vintage Story",
                    "fr": "Choisissez un nom de domaine pour Vintage Story"
                },
                "example": "example.com"
            },

            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for Vintage Story /vs is accepted",
                    "fr": "Choisissez un chemin pour Vintage Story /vs est accept√©"
                },
                "example": "/vs",
                "default": "/vs"
            },
            
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public site ?",
                    "fr": "Est-ce un site public ?"
                },
                "default": "true"
            }
        ]
    }
}
